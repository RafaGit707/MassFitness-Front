<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:centerX="10%"
                android:centerY="90%"
                android:angle="70"
                android:gradientRadius="60%"
